    "@svgr/webpack": "^2.2.1",
    "autoprefixer": "8.6.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-named-asset-import": "file:/usr/local/var/www/create-react-app/packages/babel-plugin-named-asset-import",
    "babel-preset-react-app": "file:/usr/local/var/www/create-react-app/packages/babel-preset-react-app",
    "bfj": "5.2.0",
    "bootstrap": "4.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "0.28.11",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "file:/usr/local/var/www/create-react-app/packages/eslint-config-react-app",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "1.1.11",
    "flow-bin": "^0.74.0",
    "flow-watch": "^1.1.3",
    "fs-extra": "^6.0.1",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "22.4.3",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "object-assign": "4.1.1",
