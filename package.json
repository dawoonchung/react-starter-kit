    "stylelint": "^9.0.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.0",
    "stylelint-webpack-plugin": "^0.10.1",
    "svgr": "1.9.2",
    "sw-precache-webpack-plugin": "0.11.5",
